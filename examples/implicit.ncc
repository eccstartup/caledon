infixr 0 =:=

defn =:= : {a : atom} a -> a -> atom
  as eq = [a : atom] a =:= a

let getIdent : [f : atom] identity f -> f -> atom
 be \f : atom . \id : identity f . \v : f . ((=:=) : identity f -> identity f -> atom ) (cons v) id

 

